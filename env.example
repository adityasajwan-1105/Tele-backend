# Backend Environment Variables
# Copy this file to .env and fill in your actual values

# MongoDB Connection String
# Get this from: https://cloud.mongodb.com/
MONGODB_URI=mongodb+srv://username:password@cluster0.xxxxx.mongodb.net/telemedicine?retryWrites=true&w=majority

# JWT Secret Key (use a long random string)
# Generate one at: https://randomkeygen.com/ or use: openssl rand -base64 32
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Server Port (for local development)
PORT=5000

# Frontend URL(s) - comma-separated for multiple origins
# For Vercel: https://your-frontend-app.vercel.app
CLIENT_ORIGIN=http://localhost:5173,http://localhost:4173

